{
  "name": "airbnb-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "concurrently \"npm run watch:storybook\" \"npm run watch:tailwind\"",
    "watch:storybook": "storybook dev -p 6006",
    "watch:tailwind": "npx tailwind -i ./src/app/globals.css -o ./src/styles/tailwind-output.css --watch",
    "build-storybook": "npm build:tailwind && storybook build",
    "build:tailwind": "npx tailwind -i ./src/app/globals.css -o ./src/styles/tailwind-output.css"
  },
  "dependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.6.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "next": "13.4.6",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.9.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/nextjs": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/testing-library": "^0.0.14-next.2",
    "concurrently": "^8.2.0",
    "eslint-plugin-storybook": "^0.6.12",
    "storybook": "^7.0.22"
  }
}
